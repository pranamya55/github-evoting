{
	"name": "secure-data-manager-frontend",
	"version": "1.5.2-2",
	"author": "Swiss Post Ltd.",
	"license": "(c) Copyright 2025 Swiss Post Ltd.",
	"description": "Secure Data Manager",
	"main": "electron/main.js",
	"scripts": {
		"build": "nx run-many -t build -p sdm-launcher sdm-setup sdm-tally sdm-online --parallel=false",
		"build:without-cache": "npm run build --skip-nx-cache",
		"build:launcher": "nx build sdm-launcher",
		"build:setup": "nx build sdm-setup",
		"build:online": "nx build sdm-online",
		"build:tally": "nx build sdm-tally",
		"package:electron": "run-script-os",
		"package:electron:darwin": "USE_HARD_LINKS=false BUILD_NUMBER=0 electron-builder build -mw --publish=never",
		"package:electron:linux": "USE_HARD_LINKS=false BUILD_NUMBER=0 electron-builder build -w --publish=never",
		"package:electron:win32": "electron-builder build -w --publish=never",
		"start": "nx run-many -t serve -p sdm-setup sdm-tally sdm-online",
		"start:launcher": "nx serve sdm-launcher",
		"start:setup": "nx serve sdm-setup",
		"start:online": "nx serve sdm-online",
		"start:tally": "nx serve sdm-tally",
		"start:electron": "electron ./electron/main.js",
		"test": "nx test shared-util-testing && nx run-many -t test --all --exclude shared-util-testing shared-data-dummy",
		"lint": "nx run-many -t lint --all",
		"lint:fix": "nx run-many -t lint --all --fix",
		"generate:util": "nx g @nx/js:library --tags scope:shared,type:util --directory libs/shared",
		"generate:ui": "nx g @nx/angular:library --tags scope:shared,type:ui --directory libs/shared",
		"generate:feature": "nx g @nx/angular:library --tags scope:shared,type:feature --directory libs/shared",
		"graph": "nx graph",
		"reset": "nx reset",
		"install:vendor": "npm install e-voting-libraries-ui-kit"
	},
	"private": true,
	"dependencies": {
		"@angular/animations": "20.3.7",
		"@angular/common": "20.3.7",
		"@angular/compiler": "20.3.7",
		"@angular/core": "20.3.7",
		"@angular/forms": "20.3.7",
		"@angular/localize": "20.3.7",
		"@angular/platform-browser": "20.3.7",
		"@angular/platform-browser-dynamic": "20.3.7",
		"@angular/router": "20.3.7",
		"@ng-bootstrap/ng-bootstrap": "19.0.1",
		"@ngx-translate/core": "16.0.4",
		"@ngx-translate/http-loader": "16.0.1",
		"bootstrap": "5.3.8",
		"bootstrap-icons": "1.13.1",
		"e-voting-libraries-ui-kit": "file:vendor/e-voting-libraries-ui-kit",
		"marked": "15.0.12",
		"ng-mocks": "14.14.0",
		"ngx-clipboard": "16.0.0",
		"ngx-markdown": "20.1.0",
		"ngx-mask": "19.0.7",
		"rxjs": "7.8.2",
		"tslib": "2.8.1",
		"zone.js": "0.15.1"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "20.3.7",
		"@angular-devkit/core": "20.3.7",
		"@angular-devkit/schematics": "20.3.7",
		"@angular-eslint/eslint-plugin": "20.4.0",
		"@angular-eslint/eslint-plugin-template": "20.4.0",
		"@angular-eslint/template-parser": "20.4.0",
		"@angular/cli": "20.3.7",
		"@angular/compiler-cli": "20.3.7",
		"@angular/language-service": "20.3.7",
		"@nx/angular": "21.6.6",
		"@nx/eslint-plugin": "21.6.6",
		"@nx/jest": "21.6.6",
		"@nx/js": "21.6.6",
		"@nx/workspace": "21.6.6",
		"@schematics/angular": "20.3.7",
		"@swc-node/register": "1.11.1",
		"@swc/core": "1.13.21",
		"@types/jest": "30.0.0",
		"@types/lodash": "4.17.20",
		"@types/node": "22.18.12",
		"@typescript-eslint/eslint-plugin": "8.46.2",
		"@typescript-eslint/parser": "8.46.2",
		"electron": "38.4.0",
		"electron-builder": "26.0.12",
		"eslint": "9.38.0",
		"eslint-config-prettier": "10.1.8",
		"jest": "30.2.0",
		"jest-environment-jsdom": "30.2.0",
		"jest-environment-node": "30.2.0",
		"jest-preset-angular": "15.0.3",
		"jsonc-eslint-parser": "2.4.1",
		"nx": "21.6.6",
		"prettier": "3.6.2",
		"run-script-os": "1.1.6",
		"ts-jest": "29.4.5",
		"ts-node": "10.9.2",
		"typescript": "5.9.3"
	},
	"overrides": {
		"@angular-devkit/build-angular": {
			"jest": "30.2.0",
			"jest-environment-jsdom": "30.2.0"
		},
		"js-yaml": "4.1.0"
	},
	"builders": "builders.json"
}
