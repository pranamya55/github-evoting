{
	"name": "shared-feature-shell",
	"$schema": "../../../node_modules/nx/schemas/project-schema.json",
	"sourceRoot": "libs/shared/feature-shell/src",
	"prefix": "sdm",
	"projectType": "library",
	"tags": [
		"scope:shared",
		"type:feature"
	],
	"targets": {
		"test": {
			"executor": "@nx/jest:jest",
			"outputs": [
				"{workspaceRoot}/coverage/{projectRoot}"
			],
			"options": {
				"jestConfig": "libs/shared/feature-shell/jest.config.ts",
				"passWithNoTests": true
			},
			"configurations": {
				"ci": {
					"ci": true,
					"codeCoverage": true
				}
			}
		},
		"lint": {
			"executor": "@nx/linter:eslint",
			"outputs": [
				"{options.outputFile}"
			],
			"options": {
				"lintFilePatterns": [
					"libs/shared/feature-shell/**/*.ts",
					"libs/shared/feature-shell/**/*.html"
				]
			}
		}
	}
}
