{
	"name": "launcher",
	"$schema": "../../node_modules/nx/schemas/project-schema.json",
	"sourceRoot": "libs/launcher/src",
	"prefix": "sdm",
	"projectType": "library",
	"tags": [
		"scope:launcher",
		"type:feature"
	],
	"targets": {
		"test": {
			"executor": "@nx/jest:jest",
			"outputs": [
				"{workspaceRoot}/coverage/{projectRoot}"
			],
			"options": {
				"jestConfig": "libs/launcher/jest.config.ts",
				"passWithNoTests": true
			},
			"configurations": {
				"ci": {
					"ci": true,
					"codeCoverage": true
				}
			}
		},
		"lint": {
			"executor": "@nx/linter:eslint",
			"outputs": [
				"{options.outputFile}"
			],
			"options": {
				"lintFilePatterns": [
					"libs/launcher/**/*.ts",
					"libs/launcher/**/*.html"
				]
			}
		}
	}
}
