{
	"name": "voter-portal-data-access",
	"$schema": "../../../node_modules/nx/schemas/project-schema.json",
	"sourceRoot": "libs/voter-portal/data-access/src",
	"prefix": "vp",
	"projectType": "library",
	"tags": [
		"scope:voter-portal",
		"type:data-access"
	],
	"targets": {
		"test": {
			"executor": "@nx/jest:jest",
			"outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
			"options": {
				"jestConfig": "libs/voter-portal/data-access/jest.config.ts",
				"passWithNoTests": true
			},
			"configurations": {
				"ci": {
					"ci": true,
					"codeCoverage": true
				}
			}
		},
		"lint": {
			"executor": "@nx/eslint:lint",
			"outputs": ["{options.outputFile}"],
			"options": {
				"lintFilePatterns": [
					"libs/voter-portal/data-access/**/*.ts",
					"libs/voter-portal/data-access/**/*.html"
				]
			}
		}
	}
}
