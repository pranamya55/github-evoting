{
	"name": "voter-portal-util-types",
	"$schema": "../../../node_modules/nx/schemas/project-schema.json",
	"sourceRoot": "libs/voter-portal/util-types/src",
	"projectType": "library",
	"tags": ["scope:voter-portal","type:util"],
	"targets": {
		"build": {
			"executor": "@nx/js:tsc",
			"outputs": ["{options.outputPath}"],
			"options": {
				"outputPath": "dist/libs/voter-portal/util-types",
				"main": "libs/voter-portal/util-types/src/index.ts",
				"tsConfig": "libs/voter-portal/util-types/tsconfig.lib.json",
				"assets": ["libs/voter-portal/util-types/*.md"]
			}
		},
		"lint": {
			"executor": "@nx/eslint:lint",
			"outputs": ["{options.outputFile}"],
			"options": {
				"lintFilePatterns": [
					"libs/voter-portal/util-types/**/*.ts",
					"libs/voter-portal/util-types/package.json"
				]
			}
		},
		"test": {
			"executor": "@nx/jest:jest",
			"outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
			"options": {
				"jestConfig": "libs/voter-portal/util-types/jest.config.ts",
				"passWithNoTests": true
			},
			"configurations": {
				"ci": {
					"ci": true,
					"codeCoverage": true
				}
			}
		}
	}
}
